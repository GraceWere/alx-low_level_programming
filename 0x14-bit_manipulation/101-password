#include <stdio.h>
#include <stdlib.h>

int main(void)
{
    FILE *fp;
    char *password = "Congratulations!"; 
    fp = fopen("101-password", "w");
    if (fp == NULL) {
        printf("Error opening file\n");
        return 1;
    }

    fprintf(fp, "%s", password);
    fclose(fp);

    printf("Password saved to 101-password\n");
    return 0;
}

